{
  "name": "imdb-link-em-all",
  "version": "2.0.12",
  "description": "Adds all kinds of links to IMDb, customizable!",
  "main": "imdb-link-em-all.user.js",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write \"*.{js,json}\" \"src/*.{js,json}\"",
    "watch": "rollup -c -w",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buzz/imdb-link-em-all.git"
  },
  "private": true,
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version"
  ],
  "keywords": [
    "userscript",
    "imdb",
    "browser",
    "firefox",
    "chrome",
    "greasemonkey",
    "greasyfork",
    "tampermonkey"
  ],
  "author": "buzz",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/buzz/imdb-link-em-all/issues"
  },
  "homepage": "https://github.com/buzz/imdb-link-em-all#readme",
  "config": {
    "sitesUrl": "https://raw.githubusercontent.com/buzz/imdb-link-em-all/master/sites.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "precss": "^4.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.53.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-userscript-metablock": "^0.3.0",
    "sugarss": "^4.0.1"
